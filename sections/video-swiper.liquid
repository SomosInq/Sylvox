{%- render 'section-video-swiper' -%}

{% schema %}
  {
    "name": "Section name",
    "class": "index-section",
    "settings": [
      {
        "id": "swiper_title",
        "type": "text",
        "label": "t:labels.heading",
        "default": "sylvox Community"
      },
      {
        "id": "swiper_sub_title",
        "type": "text",
        "label": "Content Text",
        "default": "A home for all Sylvox fans to experience content,inspiration, and our beautiful technology."

      },
    ],
     "blocks": [
      {
        "type": "video_block",
        "name": "Video Block",
        "settings": [
          {
            "type": "video",
            "id": "video",
            "label": "t:labels.video",
            "info": "t:info.overrides_video_url_if_both_set"
          },
          {
            "type": "video_url",
            "id": "video_url",
            "label": "t:labels.video_url",
            "default": "https://www.youtube.com/watch?v=_9VUPq3SxOc",
            "accept": ["youtube", "vimeo"]
          },
        ]
      }
    ],
    "presets": [
      {
        "name": "Video Swiper",
        "blocks": [
          {
            "type": "video_block"
          },
          {
            "type": "video_block"
          },
          {
            "type": "video_block"
          },
          {
            "type": "video_block"
          },
          {
            "type": "video_block"
          },
          {
            "type": "video_block"
          }
        ]
      }
    ],
  }
{% endschema %}